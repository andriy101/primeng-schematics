{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Adds PrimeNG to the application without affecting any templates",
      "factory": "./ng-add",
      "schema": "./ng-add/schema.json",
      "aliases": [ "primeng-add" ]
    },
    "menubar": {
      "description": "Create a menubar component",
      "factory": "./ng-generate/menubar/index",
      "schema": "./ng-generate/menubar/schema.json",
      "aliases": [ "primeng-menubar", "mb" ]
    },
    "org-chart": {
      "description": "Create an organization chart component",
      "factory": "./ng-generate/org-chart/index",
      "schema": "./ng-generate/org-chart/schema.json",
      "aliases": [ "primeng-org-chart", "oc" ]
    },
    "sidebar": {
      "description": "Create a sidebar component",
      "factory": "./ng-generate/sidebar/index",
      "schema": "./ng-generate/sidebar/schema.json",
      "aliases": [ "primeng-sidebar", "sb" ]
    },
    "table": {
      "description": "Create a table component",
      "factory": "./ng-generate/table/index",
      "schema": "./ng-generate/table/schema.json",
      "aliases": [ "primeng-table", "t" ]
    }
  }
}
