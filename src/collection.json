{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "ng-add": {
      "description": "Adds PrimeNG to the application without affecting any templates",
      "factory": "./shell",
      "schema": "./shell/schema.json",
      "aliases": [ "primeng-shell" ]
    },
    "menubar": {
      "description": "Create a menubar component",
      "factory": "./menubar",
      "schema": "./menubar/schema.json",
      "aliases": [ "primeng-menubar" ]
    },
    "orgChart": {
      "description": "Create an organization chart component",
      "factory": "./org-chart",
      "schema": "./org-chart/schema.json",
      "aliases": [ "primeng-org-chart" ]
    },
    "sidebar": {
      "description": "Create a sidebar component",
      "factory": "./sidebar",
      "schema": "./sidebar/schema.json",
      "aliases": [ "primeng-sidebar" ]
    },
    "treetable": {
      "description": "Create a tree table component",
      "factory": "./tree-table",
      "schema": "./tree-table/schema.json",
      "aliases": [ "primeng-tree-table" ]
    }
  }
}
